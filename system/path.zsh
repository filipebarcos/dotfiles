export RBENV_HOME=$HOME/.rbenv
export GOHOME=/usr/local/go
export GOPATH=$HOME/workspace/go
export JSC_HOME=/System/Library/Frameworks/JavaScriptCore.framework/Versions/A/Resources/jsc

export PATH="./bin:$RBENV_HOME/shims:/usr/local/bin:/usr/local/sbin:$HOME/bin:$HOME/.sfs:$ZSH/bin:$JSC_HOME:$GOHOME/bin:$PATH"

export MANPATH="/usr/local/man:/usr/local/mysql/man:/usr/local/git/man:$MANPATH"
